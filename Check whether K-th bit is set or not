import java.util.*;
import java.lang.*;
import java.io.*;
class GFG
 {
	public static void main (String[] args)
	 {
	 //code
	 Scanner sc=new Scanner(System.in);
	 int t=sc.nextInt();
	 while(t-->0){
	 int n=sc.nextInt();
	 int k=sc.nextInt();
	 n=n>>k;
    int lb=n&1;
	 if(lb==0){
	     System.out.println("No");
	 }else
	    System.out.println("Yes");
	 }
}
}
