import java.util.*;
import java.lang.*;
import java.io.*;
class GFG
 {
	public static void main (String[] args)
	 {
	 //code
	 Scanner sc=new Scanner(System.in);
	 int t=sc.nextInt();
	 while(t-->0){
	 int n=sc.nextInt();
	 int p=0;
	 for(int i=0;i<n;i++){
	     int num=sc.nextInt();
	     p^=num;
	 }
	 System.out.println(p);
	 }
}
}
