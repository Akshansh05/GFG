
class Queues
{
    Queue<Integer> q1 = new LinkedList<Integer>();
    Queue<Integer> q2 = new LinkedList<Integer>();
    
    /*The method pop which return the element poped out of the stack*/
    int pop()
    {
	    // Your code here
	    if(q1.size() == 0)
	     return -1;
	     else{
	         while(q1.size() !=1)
	         q2.add(q1.remove());
	     }
	     int y =q1.poll();//removes the head or the 1st element and return it
	     while(q2.size()!=0)
	     q1.add(q2.remove());
	     
	     return y;
	    
    }
	
    /* The method push to push element into the stack */
    void push(int a)
    {
	    q1.add(a);	
    }
}
