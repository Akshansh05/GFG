import java.util.*;
import java.lang.*;
import java.io.*;
class GFG
 {
	public static void main (String[] args)
	 {
	 //code
	 Scanner sc=new Scanner(System.in);
	 int t=sc.nextInt();
	 while(t-->0){
	 int a=sc.nextInt();
	 int b=sc.nextInt();
	 int x=a^b;
	 int res=0;
	 while(x!=0){
	     res+=x&1;
	     x=x>>1;
	 }
	 System.out.println(res);
	 }
}
}
