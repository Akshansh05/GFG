import java.util.*;
import java.lang.*;
import java.io.*;
class GFG
 {
	public static void main (String[] args)
	 {
	 //code
	 Scanner sc=new Scanner(System.in);
	 int t=sc.nextInt();
	 while(t-->0){
	 int n = sc.nextInt();
	 int count=0;
	 while(n!=0){
	 count+=n&1;
	 n=n>>1;
	 }
	 System.out.println(count);
	 }
}
}
