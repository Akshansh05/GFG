import java.util.*;
import java.lang.*;
import java.io.*;
class GFG
 {
	public static void main (String[] args)
	 {
	 //code
	 Scanner sc=new Scanner(System.in);
	 int t=sc.nextInt();
	 while(t-->0){
	 int n= sc.nextInt();
	 int count=0;
	 while(n!=1){
	     if(n%3 == 0){
	         n=n/3;
	     }else{
	         n--;
	     }
	     count++;
	 }
	 System.out.println(count);

	 }
}
}
