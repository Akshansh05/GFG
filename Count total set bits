import java.util.*;
import java.lang.*;
import java.io.*;
class GFG
 {
	public static void main (String[] args)
	 {
	 //code
	 Scanner sc=new Scanner(System.in);
	 int t=sc.nextInt();
	 while(t-->0){
	 int n=sc.nextInt();
	 int count=0;
	 for(int i=1;i<=n;i++){
	     int num=i;
	     while(num!=0){
	         count+=num&1;
	         num>>=1;
	     }
	 }
	 System.out.println(count);
	 }
}
}
