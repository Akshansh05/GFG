import java.util.*;
import java.lang.*;
import java.io.*;
class GFG
 {
	public static void main (String[] args)
	 {
	 //code
	 Scanner sc=new Scanner(System.in);
	 int t=sc.nextInt();
	 while(t-->0){
	 int n=sc.nextInt();
	 int count=0;
	 if(n!=0){
	 while(n!=0){
	     int lb=n&1;
	     if(lb==1){
	         break;
	     }
	     count++;
	     n=n>>1;
	 }
	 System.out.println(count+1);
	 }else
	  System.out.println(0);    
	 }
}
}
